<% if @skills.present? %>
	<ul>
	<% @skills.each do |skill| %>
		<li><%= "#{skill.description}, #{skill.skill_type}"%> <%= link_to "#{skill.user.name}", user_path(skill.user.id) %> </li>
	<% end %>
</ul>
<% else %>
	<p>There are no people with skills containing the term(s) <%= params[:search] %>.</p>
<% end %>